<h1><%= @page_title %></h1>
<div id='wrapper'>

  <% @products.each do |product| %>
  <a href='/products/location/<%= product['location'] %>'>
  <div class='product'>
    <div class='thumb'>
      <img src='<%= product['url'] %>' />
    </div>
    <div class='caption'>
      <%= titlecase(product['location']) %>
    </div>
  </div>
  </a>
  <% end %>

</div>
